<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
    <Class name="~.*\.Manifest\$.*"/>
    </Match>
    <Match>
        <Source name="~.*\.kt"/>
    </Match>
    <Match>
        <Class name="com.mongodb.stitch.core.services.mongodb.remote.sync.internal.DataSynchronizer" />
        <Bug pattern="SC_START_IN_CTOR" />
    </Match>
</FindBugsFilter>
